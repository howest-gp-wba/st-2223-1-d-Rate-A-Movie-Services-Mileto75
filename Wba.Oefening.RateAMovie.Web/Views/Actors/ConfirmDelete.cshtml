

<h2>Confirm Delete</h2>
<div class="row">
    <div class="alert alert-info" role="alert">
        Are you sure?
        <a asp-controller="Actors" asp-action="Index" class="btn btn-info">No</a>
        <a asp-controller="Actors" 
           asp-action="Delete" asp-route-Id="@ViewBag.Id" class="btn btn-danger">Yes</a>
    </div>
</div>

